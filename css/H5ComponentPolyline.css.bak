/* 折线图组件样式 */
.h5_component_polyline{}

.h5_component_polyline canvas{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color:#0062e0;
}

.h5_component_polyline .text{
   position: absolute;
    font-size: 12px;
    text-align: center;
    bottom: -20px;
    height: 20px;
    line-height: 20px;
    transform: scale(.8);
}